<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.halo.bumf.UserMapper">
	<select id="com.halo.bumf.UserMapper.selectByUserAccount" parameterType="com.halo.bumf.mvc.beans.User"
		resultType="com.halo.bumf.mvc.beans.User">
		select * from User where userAccount = #{userAccount}
	</select>
</mapper>